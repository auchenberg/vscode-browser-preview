{
  "name": "vscode-browserview",
  "description": "Browser view for VS Code",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.29.0"
  },
  "publisher": "auchenberg",
  "activationEvents": [
    "onView:nodeDependencies",
    "onCommand:browserview.showInstance"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "browserview.showInstance",
        "title": "Show Browser View",
        "category": "Browser"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "browser-view",
          "title": "Browser View",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "browser-view": [
        {
          "id": "nodeDependencies",
          "name": "Targets"
        }
      ]
    }
  },
  "configuration": {
    "title": "BrowserView",
    "type": "object",
    "properties": {
      "browserView.startUrl": {
        "type": "string",
        "default": "https://code.visualstudio.com",
        "description": "The default start url for new BrowserView instances"
      }
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "event-emitter-enhancer": "^1.0.57",
    "eventemitter2": "^5.0.1",
    "get-chrome": "0.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "lodash": "^4.17.11",
    "puppeteer-core": "^1.11.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-loading-bar": "^0.0.7",
    "react-scripts-ts": "2.16.0",
    "supports-color": "^6.0.0",
    "which-chrome": "^0.1.1"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "prettier": "./node_modules/.bin/prettier --write src/**/{*.tsx,*.ts}"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "prettier": "^1.15.3",
    "typescript": "^2.8.3",
    "vscode": "^1.1.17"
  }
}
